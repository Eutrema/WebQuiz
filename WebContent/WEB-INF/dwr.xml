<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
    "http://getahead.org/dwr//dwr30.dtd">

<dwr>
	<allow>
		<create creator="new" javascript="QuizGetter">
			<param name="class" value="jp.ac.oit.igakilab.webquiz.QuizGetter" />
		</create>

		<create creator="new" javascript="TeacherLoginManager">
			<param name="class" value="jp.ac.oit.igakilab.webquiz.TeacherLoginManager" />
		</create>

		<convert converter="bean" match="jp.ac.oit.igakilab.webquiz.*" />
		<convert converter="exception" match="java.lang.Exception" />
		<convert converter="bean" match="java.lang.StackTraceElement" />
	</allow>
</dwr>